       </view>
      </view>
      <view class="flexWrapCenter">
         <image src="http://img0.imgtn.bdimg.com/it/u=156963840,724585560&fm=21&gp=0.jpg" class="slide-image" mode="aspectFill" />
      </view>
      <view class="flexWrapBottom">
        <view class="centent">
